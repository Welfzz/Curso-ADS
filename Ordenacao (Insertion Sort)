#include <iostream>
using namespace std;

int main()
{	
	


	int vet[10];
	int full, p, j, aux, c = 10;

	

	
	for (int i = 0; i < 10; i++) {
    cout << "Entre com o " << (i+1) << "ยบ valor: ";
		cin >> full;
		vet[i] = full;
		
	}

	for (p = 1; p < 10; p++) {
		aux = vet[p];
		j = p - 1;

		while ((j >= 0) && (aux < vet[j])) {
			vet[j + 1] = vet[j];
			j--;

		}
		vet[j + 1] = aux;
	}

	for (int i = 0; i < 10; i++) {
		cout << vet[i]<< " " ;
	}
}
